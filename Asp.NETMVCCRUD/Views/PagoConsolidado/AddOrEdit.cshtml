@model ET.PagoConsolidado
@{
    Layout = null;
}

@using (Html.BeginForm("AddOrEdit", "PagoConsolidado", FormMethod.Post, new { onsubmit = "return SubmitForm(this)" }))
{
    @Html.HiddenFor(model => model.IdPagoConsolidado)
    <div class="form-group">
        @Html.LabelFor(model => model.CodigoAlumno, new { @class = "control-label" })
        @Html.EditorFor(model => model.CodigoAlumno, new { htmlAttributes = new { @class = "form-control" } })
        @Html.ValidationMessageFor(model => model.CodigoAlumno)
    </div>
    <div class="form-group">
        @Html.LabelFor(model => model.NumDeposito, new { @class = "control-label" })
        @Html.EditorFor(model => model.NumDeposito, new { htmlAttributes = new { @class = "form-control" } })
        @Html.ValidationMessageFor(model => model.NumDeposito)
    </div>
    <div class="form-group">
        @Html.LabelFor(model => model.Importe, new { @class = "control-label" })
        @Html.EditorFor(model => model.Importe, new { htmlAttributes = new { @class = "form-control" } })
    </div>
    <div class="form-group">
        @Html.LabelFor(model => model.concepto.NroConcepto, new { @class = "control-label" })
        @Html.EditorFor(model => model.concepto.NroConcepto, new { htmlAttributes = new { @class = "form-control" } })
    </div>
       <div class="form-group">
        @Html.LabelFor(model => model.FechaPago, new { @class = "control-label" })
        @Html.EditorFor(model => model.FechaPago, new { htmlAttributes = new { @class = "form-control" } })
    </div>


    <div class="form-group">
        @Html.LabelFor(model => model.CodigoMatricula, new { @class = "control-label" })
        @Html.EditorFor(model => model.CodigoMatricula, new { htmlAttributes = new { @class = "form-control" } })
    </div>



    
    <div class="form-group">
        <input type="submit" value="Submit" class="btn btn-primary"  />
        <input type="reset" value="Reset" class="btn" />
    </div>
}